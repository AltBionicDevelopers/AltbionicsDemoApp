<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:gravity="center"
        android:width="20dp"
        android:height="20dp">
        <shape
            android:shape="oval">
            <solid
                android:color="#000000" />
            <stroke
                android:width="2dp"
                android:color="?attr/primaryColor" />
        </shape>
    </item>
    <item
        android:gravity="center"
        android:width="10dp"
        android:height="10dp">
        <shape android:shape="oval">
            <solid
                android:color="#51FF01"/>
        </shape>
    </item>
</layer-list>