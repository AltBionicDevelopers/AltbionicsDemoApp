<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="55dp"
        android:height="55dp">
        <shape android:shape="oval">
            <solid
                android:color="#FFFFFFFF" />
            <stroke
                android:width="2dp"
                android:color="?attr/primaryColor"/>
        </shape>
    </item>

    <item
        android:width="6dp"
        android:height="20dp"
        android:gravity="center">
        <shape android:shape="rectangle">
            <solid
                android:color="?attr/primaryColor" />
        </shape>
    </item>

    <item
        android:width="20dp"
        android:height="6dp"
        android:gravity="center">
        <shape android:shape="rectangle">
            <solid
                android:color="?attr/primaryColor" />
        </shape>
    </item>
</layer-list>